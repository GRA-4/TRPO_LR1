                    <Image Source="/Angry_emotion.png"  Visibility="Collapsed" x:Name="AngryImage" />
                    <Image Source="https://images.genius.com/a3c775785c08bb746278232194792f6d.750x750x1.jpg" Visibility="Collapsed"/>
                </Grid>
            </StackPanel>
        </StackPanel>
